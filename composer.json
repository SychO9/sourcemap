{
    "name": "axy/sourcemap",
    "type": "library",
    "description": "Work with JavaScript/CSS Source Map",
    "keywords": [
        "sourcemap",
        "Source Map",
        "JavaScript",
        "CSS"
    ],
    "homepage": "https://github.com/axypro/sourcemap",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Grigoriev",
            "email": "go.vasac@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "axy/errors": "^1.0.1",
        "axy/codecs-base64vlq": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "axy/errors": "^1.x-dev",
        "axy/codecs-base64vlq": "^1.x-dev"
    },
    "autoload": {
        "psr-4": {
            "axy\\sourcemap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "axy\\sourcemap\\tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../*/"
        }
    ],
    "scripts": {
        "test": "phpunit -c phpunit.xml.dist",
        "md": "phpmd --exclude vendor . text phpmd.xml.dist",
        "cs": "phpcs --standard=PSR2 --encoding=utf-8 --ignore=vendor ."
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    }
}
